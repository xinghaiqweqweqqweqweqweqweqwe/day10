/* 水平间距 */
.homea input {
  height: 30px;
  width: 80%;
  border-radius: 30px;
  background-color: #f6f8fa;
  margin-left: 10%;
  padding-left: 15px;
}
.swiperList {
  width: 100%;
  height: 150px;
}
._h2 {
  text-align: center;
  margin-top: 10px;
}
.resultList {
  display: flex;
  flex-wrap: wrap;
}
.resultList .resultLista {
  width: 45%;
  margin: 5px;
  border: 1px solid gray;
  border-radius: 5px;
}
.resultList .resultLista image {
  width: 100%;
}
.resultList .resultLista .aa {
  color: red;
}
.resultList .resultLista .bb {
  color: gray;
  text-decoration: line-through;
  margin-left: 15px;
}
.search {
  background-color: white;
  height: 100rpx;
  display: flex;
}
.search ._span {
  font-size: 40rpx;
  font-weight: 800;
  padding: 0rpx 40rpx;
}
.search input {
  background-color: #f6f8fa;
}

